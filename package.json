    "@swc/core": "^1.2.106",
    "@swc/jest": "^0.2.5",
    "binance-api-node": "^0.11.22",
    "chalk": "^4.1.2",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "inversify": "^6.0.1",
