    "build": "rimraf ./build && npx tsc",
    "test": "jest --no-cache --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SockTrader/Binance-proxy-server.git"
  },
  "keywords": [
    "binance",
    "proxy",
    "server",
    "papertrading",
    "trading",
    "bitcoin",
    "cryptocurrency",
    "exchange"
  ],
