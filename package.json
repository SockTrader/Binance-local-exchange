    "@types/jest": "^27.0.2",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^8.2.0",
    "concurrently": "^6.3.0",
    "jest": "^27.3.1",
    "jest-extended": "^1.1.0",
    "nodemon": "^2.0.13",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
