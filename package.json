{
  "name": "binance-proxy-server",
  "version": "0.0.1",
  "description": "Binance proxy server",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:prod": "npm run build @@ node ./dist/src/index.js",
    "start:test": "concurrently -c \"bgBlue.bold,bgMagenta.bold\" -k -n proxy,socktrader \"npm run start\" \"sleep 5 && cd ../socktrader2 && npm run start\"",
